{
  "name": "fhir-package-a2a-agent",
  "version": "1.0.0",
  "description": "A2A Agent wrapper for FHIR Package Agent with TypeScript and ArkType",
  "type": "module",
  "main": "agent.ts",
  "scripts": {
    "server": "bun run server.ts",
    "client": "bun run client.ts",
    "build:fhir-agent": "cd ../.. && dotnet publish fhir-package-agent.csproj -c Release -o bin",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fhir",
    "a2a",
    "agent",
    "healthcare",
    "bun",
    "typescript",
    "arktype"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@a2a-js/sdk": "^0.1.0",
    "arktype": "^2.0.0-rc.13",
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0"
  },
  "engines": {
    "bun": ">=1.3.0"
  }
}
